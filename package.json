{
  "name": "weakauras-companion",
  "version": "5.2.2",
  "description": "WeakAuras Companion App",
  "author": "Buds <mrbouyou@gmail.com>",
  "main": "dist-electron/main/index.js",
  "type": "module",
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/WeakAuras/WeakAuras-Companion.git"
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "clean": "git clean -xdf node_modules dist dist-electron",
    "compile-tools": "tsc -p tools",
    "dev": "vite",
    "i18n": "pnpm run compile-tools && node ./tools/scripts/extract-i18n-messages.js",
    "i18n-report": "pnpm dlx vue-i18n-extract report --remove --vueFiles './src/components/**/*.?(js|vue)' --languageFiles './i18n/*.json'",
    "lint": "eslint . --cache --ext .ts,.js,.vue && prettier . --check --cache",
    "lint:fix": "eslint . --ext .ts,.js,.vue --fix && prettier . --write",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.?(js|ts|ts|vue)": "prettier --write"
  },
  "config": {
    "default-locale": "en",
    "supported-locales": [
      "en",
      "es",
      "de",
      "fr",
      "ru",
      "tr",
      "zh-cn"
    ]
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@8.12.1",
  "dependencies": {
    "archiver": "^6.0.1",
    "got": "^12.6.1",
    "regedit": "^5.1.2",
    "sharp": "^0.32.6",
    "tga": "^1.0.7"
  },
  "devDependencies": {
    "@iconify-json/fa6-brands": "^1.1.18",
    "@iconify-json/mdi": "^1.1.64",
    "@iconify/tools": "^4.0.0",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@types/archiver": "^6.0.2",
    "@types/luaparse": "0.2.1",
    "@types/luxon": "^3.4.0",
    "@types/node": "^20.11.0",
    "@types/tail": "^2.2.3",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@unocss/eslint-config": "^0.58.3",
    "@unocss/preset-icons": "^0.58.3",
    "@unocss/preset-web-fonts": "^0.58.3",
    "@vitejs/plugin-vue": "^5.0.3",
    "browserslist": "^4.22.2",
    "electron": "28.1.3",
    "electron-builder": "^24.11.0",
    "electron-log": "^5.0.3",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-vite": "^1.3.2",
    "eslint-plugin-import": "npm:eslint-plugin-i@2.29.0",
    "eslint-plugin-vue": "^9.20.1",
    "floating-vue": "^5.0.3",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "lightningcss": "^1.22.1",
    "lint-staged": "^15.2.0",
    "luaparse": "^0.2.1",
    "luxon": "^3.4.4",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate-2": "^2.0.14",
    "prettier": "^3.2.1",
    "tail": "^2.2.6",
    "tree-kill": "^1.2.2",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "vite": "^5.0.11",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-resolve": "^2.5.1",
    "vite-plugin-vue-devtools": "^7.0.8",
    "vite-plugin-webfont-dl": "^3.9.1",
    "vitest": "^1.2.0",
    "vue": "^3.4.13",
    "vue-i18n": "^9.9.0",
    "vue-tsc": "^1.8.27"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue",
    "WeakAuras",
    "World of Warcraft"
  ]
}
