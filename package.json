{
  "name": "weakauras-companion",
  "version": "5.0.0",
  "description": "WeakAuras Companion App",
  "author": "Buds <mrbouyou@gmail.com>",
  "main": "dist/main/index.cjs",
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/WeakAuras/WeakAuras-Companion.git"
  },
  "config": {
    "default-locale": "en",
    "supported-locales": [
      "en",
      "es",
      "de",
      "fr",
      "ru",
      "tr",
      "zh-cn"
    ]
  },
  "scripts": {
    "compile-tools": "tsc -p tools",
    "i18n": "node ./tools/scripts/extract-i18n-messages.js",
    "lint": "eslint packages/. --ext .js,.vue && npm run lint:scss",
    "lint:scss": "npx stylelint \"packages/**/*.scss\"",
    "dev": "node scripts/watch.mjs",
    "prebuild": "vue-tsc --noEmit --p packages/renderer/tsconfig.json && node scripts/build.mjs",
    "build": "electron-builder",
    "init": "git config core.hooksPath .git/hooks/ && rm -rf .git/hooks && npx simple-git-hooks",
    "test:e2e": "npx playwright test",
    "test:e2e:headless": "npx playwright test --headed"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@electron/remote": "^2.0.8",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@playwright/test": "^1.22.2",
    "@types/archiver": "^5.3.1",
    "@types/auto-launch": "^5.0.2",
    "@types/electron-localshortcut": "^3.1.0",
    "@types/jest": "^28.1.1",
    "@types/luaparse": "0.2.1",
    "@types/luxon": "^2.3.2",
    "@types/sharp": "^0.30.4",
    "@types/tail": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^10.0.0",
    "archiver": "^5.3.1",
    "auto-launch": "^5.0.5",
    "axios": "^0.27.2",
    "electron": "^19.0.4",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^4.4.7",
    "electron-store": "^8.0.2",
    "electron-updater": "^5.0.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-vite": "^1.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "floating-vue": "^2.0.0-beta.17",
    "glob": "^8.0.3",
    "luaparse": "0.2.1",
    "luxon": "^2.4.0",
    "material-icons": "^1.11.1",
    "nano-staged": "^0.8.0",
    "pinia": "^2.0.14",
    "pinia-plugin-persistedstate-2": "^1.0.0",
    "prettier": "^2.6.2",
    "regedit": "^5.1.1",
    "sass": "^1.52.3",
    "sharp": "^0.30.6",
    "simple-git-hooks": "^2.8.0",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended-scss": "^6.0.0",
    "tail": "^2.2.4",
    "tga": "^1.0.7",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vite-plugin-electron": "^0.4.8",
    "vite-plugin-eslint": "^1.6.1",
    "vite-plugin-resolve": "^2.1.2",
    "vue": "^3.2.37",
    "vue-i18n": "^9.1.10",
    "vue-tsc": "^0.37.3"
  },
  "external": [
    "sharp"
  ],
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue",
    "WeakAuras",
    "World of Warcraft"
  ]
}