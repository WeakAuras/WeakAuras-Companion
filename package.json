{
  "name": "weakauras-companion",
  "version": "5.2.10",
  "description": "WeakAuras Companion App",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue",
    "WeakAuras",
    "World of Warcraft"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WeakAuras/WeakAuras-Companion.git"
  },
  "license": "GPL-2.0-or-later",
  "author": "Buds <mrbouyou@gmail.com>",
  "type": "module",
  "main": "dist-electron/main/index.mjs",
  "scripts": {
    "build": "vue-tsc && vite build && electron-builder",
    "clean": "git clean -xdf node_modules dist dist-electron",
    "compile-tools": "tsc -p tools",
    "dev": "vite",
    "i18n": "pnpm run compile-tools && node ./tools/scripts/extract-i18n-messages.js",
    "i18n-report": "pnpm dlx vue-i18n-extract report --add --remove --separator 'buggy' --vueFiles './src/**/*.?(ts|vue)' --languageFiles './i18n/*.json'",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint ./src --cache && prettier . --check --cache",
    "lint:fix": "eslint ./src --fix && prettier . --write",
    "prepare": "husky",
    "test": "vitest run",
    "test:ui": "vitest --ui"
  },
  "lint-staged": {
    "*.?(js|ts|ts|vue)": [
      "prettier --write --cache",
      "eslint --cache"
    ]
  },
  "config": {
    "default-locale": "en",
    "supported-locales": [
      "en",
      "es",
      "de",
      "fr",
      "ru",
      "tr",
      "zh-cn"
    ]
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "archiver-utils": "^5.0.2",
    "got": "^12.6.1",
    "regedit": "^5.1.4",
    "sharp": "^0.33.5",
    "tga": "^1.0.7"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@iconify-json/fa6-brands": "^1.2.5",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify/tools": "^4.1.2",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@types/archiver": "^6.0.4",
    "@types/luaparse": "0.2.1",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.9.2",
    "@types/tail": "^2.2.3",
    "@unocss/eslint-config": "^66.5.4",
    "@unocss/preset-icons": "^66.5.4",
    "@unocss/preset-web-fonts": "^66.5.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser": "^4.0.3",
    "@vitest/ui": "^4.0.3",
    "browserslist": "^4.25.1",
    "electron": "38.4.0",
    "electron-builder": "^25.1.8",
    "electron-log": "^5.4.2",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.6.2",
    "eslint": "^9.38.0",
    "eslint-plugin-vue": "^10.5.1",
    "floating-vue": "^5.2.2",
    "glob": "^11.0.3",
    "happy-dom": "^20.0.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "luaparse": "^0.2.1",
    "luxon": "^3.7.1",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate-2": "^2.0.32",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "tail": "^2.2.6",
    "tree-kill": "^1.2.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.46.2",
    "unocss": "^66.5.4",
    "vite": "^7.1.12",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-resolve": "^2.5.2",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vite-plugin-webfont-dl": "^3.11.1",
    "vitest": "^4.0.3",
    "vitest-browser-vue": "^2.0.0",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12",
    "vue-tsc": "^3.1.2"
  },
  "packageManager": "pnpm@10.18.1",
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild",
      "sharp",
      "vue-demi"
    ],
    "supportedArchitectures": {
      "cpu": [
        "x64",
        "arm64"
      ],
      "os": [
        "win32",
        "darwin",
        "linux"
      ]
    }
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  }
}
