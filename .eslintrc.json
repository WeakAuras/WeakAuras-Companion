{
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "plugin:vue/essential"
    ],
    "globals": {
        "__static": true
    },
    "rules": {
        "linebreak-style": "off",
        "no-console": "off",
        "semi": 0
    },
    "settings": {
        "import/core-modules": [ "electron" ],
        "import/resolver": {
            "webpack": {
                "config": "./.electron-vue/webpack.renderer.config.js"
            }
        }
    }
}