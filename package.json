{
  "name": "weakauras-companion",
  "version": "5.2.10",
  "description": "WeakAuras Companion App",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue",
    "WeakAuras",
    "World of Warcraft"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WeakAuras/WeakAuras-Companion.git"
  },
  "license": "GPL-2.0-or-later",
  "author": "Buds <mrbouyou@gmail.com>",
  "type": "module",
  "main": "dist-electron/main/index.mjs",
  "scripts": {
    "build": "vue-tsc && vite build && electron-builder",
    "clean": "git clean -xdf node_modules dist dist-electron",
    "compile-tools": "tsc -p tools",
    "dev": "vite",
    "i18n": "pnpm run compile-tools && node ./tools/scripts/extract-i18n-messages.js",
    "i18n-report": "pnpm dlx vue-i18n-extract report --add --remove --separator 'buggy' --vueFiles './src/**/*.?(ts|vue)' --languageFiles './i18n/*.json'",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint ./src --cache && prettier . --check --cache",
    "lint:fix": "eslint ./src --fix && prettier . --write",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.?(js|ts|ts|vue)": [
      "prettier --write --cache",
      "eslint --cache"
    ]
  },
  "config": {
    "default-locale": "en",
    "supported-locales": [
      "en",
      "es",
      "de",
      "fr",
      "ru",
      "tr",
      "zh-cn"
    ]
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "archiver-utils": "^5.0.2",
    "got": "^12.6.1",
    "regedit": "^5.1.4",
    "sharp": "^0.33.5",
    "tga": "^1.0.7"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@iconify-json/fa6-brands": "^1.2.5",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify/tools": "^4.1.2",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@types/archiver": "^6.0.3",
    "@types/luaparse": "0.2.1",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.15.29",
    "@types/tail": "^2.2.3",
    "@unocss/eslint-config": "^65.4.3",
    "@unocss/preset-icons": "^65.4.3",
    "@unocss/preset-web-fonts": "^65.4.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "browserslist": "^4.25.1",
    "electron": "35.1.2",
    "electron-builder": "^25.1.8",
    "electron-log": "^5.4.0",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.6.2",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^9.33.0",
    "floating-vue": "^5.2.2",
    "glob": "^11.0.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "luaparse": "^0.2.1",
    "luxon": "^3.6.1",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate-2": "^2.0.30",
    "prettier": "^3.4.2",
    "tail": "^2.2.6",
    "tree-kill": "^1.2.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "8.33.0",
    "unocss": "^65.4.3",
    "vite": "^5.4.19",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-resolve": "^2.5.2",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vite-plugin-webfont-dl": "^3.10.4",
    "vitest": "^3.1.4",
    "vue": "^3.5.17",
    "vue-i18n": "^11.1.5",
    "vue-tsc": "^2.2.10"
  },
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "supportedArchitectures": {
      "cpu": [
        "x64",
        "arm64"
      ],
      "os": [
        "win32",
        "darwin",
        "linux"
      ]
    }
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  }
}
