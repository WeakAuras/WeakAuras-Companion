{
  "name": "weakauras-companion",
  "version": "3.3.4",
  "description": "WeakAuras Companion App",
  "author": "Buds <mrbouyou@gmail.com>",
  "scripts": {
    "build": "vue-cli-service electron:build --publish never",
    "lint": "eslint src/. --ext .js,.vue",
    "compile-tools": "cd node_modules/.bin/ && tsc -p ../../tools/",
    "dev": "vue-cli-service electron:serve",
    "dist": "vue-cli-service electron:build --publish onTagOrDraft",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "i18n": "npm run compile-tools && node ./tools/scripts/extract-i18n-messages.js",
    "lint:scss": "npx stylelint \"src/**/*.vue\"",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@types/jest": "^27.0.1",
    "archiver": "^5.3.0",
    "auto-launch": "^5.0.5",
    "axios": "^0.21.4",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^4.4.1",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.9",
    "luaparse": "~0.2.1",
    "luxon": "^2.0.2",
    "regedit": "^5.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "tail": "^2.2.3",
    "v-tooltip": "^2.1.3",
    "vue": "^2.6.14",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.25.0",
    "vue-toasted": "^1.1.28"
  },
  "devDependencies": {
    "@vue/cli": "^4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-service": "~4.5.13",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.2.7",
    "devtron": "^1.4.0",
    "electron": "^13.2.3",
    "electron-builder": "^22.11.7",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "next",
    "glob": "^7.1.7",
    "html-webpack-plugin": "^4.5.2",
    "prettier": "^2.4.1",
    "sass": "^1.39.2",
    "sass-loader": "^10.1.1",
    "typescript": "^4.4.3",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-cli-plugin-esbuild": "~0.0.4",
    "vue-loader": "^16.5.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "Electron >= 12.0.0"
  ],
  "config": {
    "default-locale": "en",
    "supported-locales": [
      "en",
      "es",
      "de",
      "fr",
      "ru",
      "tr",
      "zh-cn"
    ]
  },
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/WeakAuras/WeakAuras-Companion.git"
  }
}
