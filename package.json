{
  "name": "weakauras-companion",
  "version": "4.0.0-beta1",
  "description": "WeakAuras Companion App",
  "author": "Buds <mrbouyou@gmail.com>",
  "scripts": {
    "build": "vue-cli-service electron:build --publish never",
    "lint": "eslint src/. --ext .js,.vue",
    "compile-tools": "cd node_modules/.bin/ && tsc -p ../../tools/",
    "dev": "vue-cli-service electron:serve",
    "dev:app": "vue-cli-service serve",
    "dist": "vue-cli-service electron:build --publish onTagOrDraft",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "i18n": "npm run compile-tools && node ./tools/scripts/extract-i18n-messages.js",
    "lint:scss": "npx stylelint \"src/**/*.vue\"",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@types/jest": "^27.4.1",
    "@vue/compat": "^3.2.31",
    "archiver": "^5.3.0",
    "auto-launch": "^5.0.5",
    "axios": "^0.26.1",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^4.4.6",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.6.5",
    "floating-vue": "^2.0.0-beta.12",
    "luaparse": "~0.2.1",
    "luxon": "^2.3.1",
    "pinia": "^2.0.13",
    "pinia-plugin-persistedstate-2": "^1.0.0",
    "regedit": "^5.0.0",
    "stylelint": "^14.6.1",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-scss": "^4.2.0",
    "tail": "^2.2.4",
    "vue": "^3.2.31",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^9.2.0-beta.30"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.1",
    "@types/jest": "^27.4.1",
    "@types/luaparse": "^0.2.8",
    "@types/luxon": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vue/cli": "^5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-plugin-typescript": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/eslint-config-typescript": "^10.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "electron": "^18.0.1",
    "electron-builder": "^22.14.13",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "glob": "^7.2.0",
    "html-webpack-plugin": "^4.5.2",
    "prettier": "^2.6.2",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "vue-cli-plugin-electron-builder": "^3.0.0-alpha.4",
    "vue-loader": "^17.0.0",
    "vue-style-loader": "^4.1.3",
