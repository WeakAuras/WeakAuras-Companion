{
  "name": "weakauras-companion",
  "version": "5.1.3",
  "description": "WeakAuras Companion App",
  "author": "Buds <mrbouyou@gmail.com>",
  "main": "dist-electron/main/index.js",
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/WeakAuras/WeakAuras-Companion.git"
  },
  "scripts": {
    "compile-tools": "tsc -p tools",
    "format:prettier": "prettier --write \"src/**/*.?(js|ts|vue|json)\"",
    "i18n": "pnpm run compile-tools && node ./tools/scripts/extract-i18n-messages.js",
    "i18n-report": "pnpm dlx vue-i18n-extract report --remove --vueFiles './src/components/**/*.?(js|vue)' --languageFiles './i18n/*.json'",
    "lint": "eslint . --ext .ts,.js,.vue && pnpm run lint:scss && pnpm run lint:prettier",
    "lint:fix": "eslint . --ext .ts,.js,.vue --fix && pnpm run format:prettier",
    "lint:prettier": "prettier --check src",
    "lint:scss": "stylelint --color \"src/**/*.scss\"",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "preinstall": "npx only-allow pnpm"
  },
  "config": {
    "default-locale": "en",
    "supported-locales": [
      "en",
      "es",
      "de",
      "fr",
      "ru",
      "tr",
      "zh-cn"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.8.0",
  "dependencies": {
    "archiver": "^6.0.1",
    "got": "^12.6.0",
    "regedit": "^5.1.2",
    "sharp": "^0.32.6",
    "tga": "^1.0.7"
  },
  "devDependencies": {
    "@iconify-json/fa6-brands": "^1.1.17",
    "@iconify-json/mdi": "^1.1.60",
    "@iconify/tools": "^3.0.7",
    "@intlify/unplugin-vue-i18n": "^1.5.0",
    "@types/archiver": "^6.0.2",
    "@types/auto-launch": "^5.0.5",
    "@types/jest": "^29.5.10",
    "@types/luaparse": "0.2.1",
    "@types/luxon": "^3.3.6",
    "@types/node": "^20.10.3",
    "@types/tail": "^2.2.3",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@unocss/preset-icons": "^0.58.0",
    "@unocss/preset-web-fonts": "^0.58.0",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "electron": "27.1.3",
    "electron-builder": "^24.9.1",
    "electron-log": "^5.0.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.7",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-vite": "^1.3.2",
    "eslint-plugin-import": "npm:eslint-plugin-i@2.27.5-3",
    "eslint-plugin-vue": "^9.19.2",
    "floating-vue": "^2.0.0-beta.24",
    "glob": "^10.3.10",
    "luaparse": "^0.2.1",
    "luxon": "^3.4.4",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate-2": "^2.0.14",
    "prettier": "4.0.0-alpha.4",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended-scss": "^13.1.0",
    "tail": "^2.2.6",
    "typescript": "^5.3.2",
    "unocss": "^0.58.0",
    "vite": "^5.0.7",
    "vite-plugin-electron": "^0.15.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-resolve": "^2.5.1",
    "vite-plugin-vue-devtools": "^1.0.0-rc.7",
    "vue": "^3.3.9",
    "vue-i18n": "^9.8.0",
    "vue-tsc": "^1.8.24"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue",
    "WeakAuras",
    "World of Warcraft"
  ]
}
