{
  "name": "weakauras-companion",
  "version": "3.1.1",
  "description": "WeakAuras Companion App",
  "author": "Buds <mrbouyou@gmail.com>",
  "scripts": {
    "build": "vue-cli-service electron:build --publish never",
    "lint": "eslint src/. --ext .js,.vue",
    "compile-tools": "./node_modules/.bin/tsc -p ./tools/",
    "dev": "vue-cli-service electron:serve",
    "dist": "npm run build --publish onTagOrDraft",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "i18n": "npm run compile-tools && node ./tools/scripts/extract-i18n-messages.js",
    "lint:scss": "npx stylelint \"src/**/*.vue\"",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@types/jest": "^26.0.19",
    "archiver": "^5.2.0",
    "auto-launch": "^5.0.5",
    "axios": "^0.21.1",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^4.3.1",
    "electron-store": "^6.0.1",
    "electron-updater": "^4.3.5",
    "luaparse": "~0.2.1",
    "luxon": "^1.25.0",
    "regedit": "^3.0.3",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "tail": "^2.2.0",
    "v-tooltip": "^2.1.1",
    "vue": "^2.6.12",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.22.3",
    "vue-toasted": "^1.1.28"
  },
  "devDependencies": {
    "@vue/cli": "^4.5.10",
    "@vue/cli-plugin-eslint": "~4.5.7",
    "@vue/cli-service": "~4.5.10",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.0.1",
    "devtron": "^1.4.0",
    "electron": "^11.1.1",
    "electron-builder": "^22.10.4",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "next",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.5.1",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.0",
    "typescript": "^4.1.3",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.5",
    "vue-loader": "^16.1.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.45.0",
    "webpack-cli": "^4.3.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "Electron >= 11.0.0"
  ],
  "config": {
    "default-locale": "en",
    "supported-locales": [
      "en",
      "es",
      "de",
      "fr",
      "ru",
      "tr",
      "zh-cn"
    ]
  },
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/WeakAuras/WeakAuras-Companion.git"
  }
}
