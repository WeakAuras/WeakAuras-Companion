{
  "name": "weakauras-companion",
  "version": "5.2.6",
  "description": "WeakAuras Companion App",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue",
    "WeakAuras",
    "World of Warcraft"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/WeakAuras/WeakAuras-Companion.git"
  },
  "license": "GPL-2.0-or-later",
  "author": "Buds <mrbouyou@gmail.com>",
  "type": "module",
  "main": "dist-electron/main/index.mjs",
  "scripts": {
    "build": "vue-tsc && vite build && electron-builder",
    "clean": "git clean -xdf node_modules dist dist-electron",
    "compile-tools": "tsc -p tools",
    "dev": "vite",
    "i18n": "pnpm run compile-tools && node ./tools/scripts/extract-i18n-messages.js",
    "i18n-report": "pnpm dlx vue-i18n-extract report --add --remove --separator 'buggy' --vueFiles './src/**/*.?(ts|vue)' --languageFiles './i18n/*.json'",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint ./src --cache && prettier . --check --cache",
    "lint:fix": "eslint ./src --fix && prettier . --write",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.?(js|ts|ts|vue)": "prettier --write"
  },
  "config": {
    "default-locale": "en",
    "supported-locales": [
      "en",
      "es",
      "de",
      "fr",
      "ru",
      "tr",
      "zh-cn"
    ]
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "got": "^12.6.1",
    "regedit": "^5.1.3",
    "sharp": "^0.32.6",
    "tga": "^1.0.7"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@iconify-json/fa6-brands": "^1.2.0",
    "@iconify-json/mdi": "^1.2.0",
    "@iconify/tools": "^4.0.5",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@types/archiver": "^6.0.2",
    "@types/luaparse": "0.2.1",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.0.2",
    "@types/tail": "^2.2.3",
    "@unocss/eslint-config": "^0.61.9",
    "@unocss/preset-icons": "^0.61.9",
    "@unocss/preset-web-fonts": "^0.61.9",
    "@vitejs/plugin-vue": "^5.1.1",
    "browserslist": "^4.23.3",
    "electron": "31.0.1",
    "electron-builder": "^24.13.3",
    "electron-log": "^5.1.7",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.2.1",
    "eslint": "^9.8.0",
    "eslint-plugin-vue": "^9.27.0",
    "floating-vue": "^5.2.2",
    "glob": "^11.0.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "luaparse": "^0.2.1",
    "luxon": "^3.4.4",
    "pinia": "^2.2.0",
    "pinia-plugin-persistedstate-2": "^2.0.23",
    "prettier": "^3.3.3",
    "tail": "^2.2.6",
    "tree-kill": "^1.2.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "8.3.0",
    "unocss": "^0.61.9",
    "vite": "^5.3.5",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-resolve": "^2.5.2",
    "vite-plugin-vue-devtools": "^7.3.7",
    "vite-plugin-webfont-dl": "^3.9.5",
    "vitest": "^2.0.5",
    "vue": "^3.4.35",
    "vue-i18n": "^9.14.0",
    "vue-tsc": "^2.1.6"
  },
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": ">=22"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  }
}
