{
  "name": "weakauras-companion",
  "version": "2.0.0",
  "description": "WeakAuras Companion App",
  "author": "Buds <mrbouyou@gmail.com>",
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/WeakAuras/WeakAuras-Companion.git"
  },
  "scripts": {
    "build": "vue-cli-service electron:build",
    "lint": "eslint src/.",
    "compile-tools": "./node_modules/.bin/tsc -p ./tools/",
    "dev": "vue-cli-service electron:serve",
    "dist": "yarn build --publish onTagOrDraft",
    "dist:pr": "yarn build --publish=never",
    "i18n": "npm run compile-tools && node ./tools/scripts/extract-i18n-messages.js",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "config": {
    "default-locale": "en",
    "supported-locales": [
      "en",
      "es",
      "de",
      "fr",
      "ru",
      "tr",
      "zh-cn"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "archiver": "^3.1.1",
    "auto-launch": "^5.0.5",
    "axios": "^0.19.0",
    "core-js": "^3.4.0",
    "electron-localshortcut": "^3.2.0",
    "electron-log": "^3.0.9",
    "electron-store": "^5.0.0",
    "electron-updater": "^4.2.0",
    "luaparse": "^0.2.1",
    "moment": "^2.24.0",
    "regedit": "^3.0.3",
    "tail": "^2.0.3",
    "v-tooltip": "^2.0.2",
    "vue": "^2.6.10",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.14.1",
    "vue-toasted": "^1.1.27"
  },
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "devtron": "^1.4.0",
    "electron": "^7.1.1",
    "electron-debug": "^3.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.0",
    "typescript": "^3.7.2",
    "vue-cli-plugin-electron-builder": "^1.4.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
