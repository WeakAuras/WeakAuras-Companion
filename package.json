{
  "name": "weakauras-companion",
  "version": "3.0.3",
  "description": "WeakAuras Companion App",
  "author": "Buds <mrbouyou@gmail.com>",
  "scripts": {
    "build": "vue-cli-service electron:build",
    "lint": "eslint src/. --ext .js,.vue",
    "compile-tools": "./node_modules/.bin/tsc -p ./tools/",
    "dev": "vue-cli-service electron:serve",
    "dist": "yarn build --publish onTagOrDraft",
    "dist:pr": "yarn build --publish=never",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "i18n": "yarn compile-tools && node ./tools/scripts/extract-i18n-messages.js",
    "lint:scss": "npx stylelint \"src/**/*.vue\"",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@types/jest": "^26.0.4",
    "archiver": "^4.0.1",
    "auto-launch": "^5.0.5",
    "axios": "^0.19.2",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^4.2.2",
    "electron-store": "^5.2.0",
    "electron-updater": "^4.3.1",
    "luaparse": "~0.2.1",
    "moment": "^2.27.0",
    "regedit": "^3.0.3",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "tail": "^2.0.4",
    "v-tooltip": "^2.0.3",
    "vue": "^2.6.11",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.19.0",
    "vue-toasted": "^1.1.28"
  },
  "devDependencies": {
    "@vue/cli": "^4.4.6",
    "@vue/cli-plugin-eslint": "~4.4.6",
    "@vue/cli-service": "~4.4.6",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "devtron": "^1.4.0",
    "electron": "^9.1.0",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.3.0",
    "node-sass": "^4.14.0",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.2",
    "typescript": "^3.9.6",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.4",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^16.0.0-beta.4",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "Electron >= 8.0.0"
  ],
  "config": {
    "default-locale": "en",
    "supported-locales": [
      "en",
      "es",
      "de",
      "fr",
      "ru",
      "tr",
      "zh-cn"
    ]
  },
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/WeakAuras/WeakAuras-Companion.git"
  }
}
