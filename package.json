{
  "name": "weakauras-companion",
  "version": "1.2.4",
  "author": "Buds <mrbouyou@gmail.com>",
  "description": "WeakAuras Companion App",
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/WeakAuras/WeakAuras-Companion.git"
  },
  "main": "./dist/electron/main.js",
  "scripts": {
    "compile-tools": "./node_modules/.bin/tsc -p ./tools/",
    "i18n": "npm run compile-tools && node ./tools/scripts/extract-i18n-messages.js",
    "build": "node .electron-vue/build.js && electron-builder",
    "dev": "node .electron-vue/dev-runner.js",
    "dist": "yarn build --publish onTagOrDraft",
    "lint": "eslint --format node_modules/eslint-formatter-friendly src/."
  },
  "build": {
    "productName": "WeakAuras Companion",
    "appId": "wtf.weakauras.companion",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "publish": {
        "provider": "github"
      },
      "extraResources": [
        "node_modules/regedit/vbs/*"
      ]
    },
    "nsis": {
      "deleteAppDataOnUninstall": true
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "config": {
    "default-locale": "en",
    "supported-locales": [
      "en",
      "de",
      "fr",
      "ru",
      "tr",
      "zh-cn"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "archiver": "^3.1.1",
    "auto-launch": "^5.0.5",
    "axios": "^0.19.0",
    "electron-localshortcut": "^3.1.0",
    "electron-log": "^3.0.8",
    "electron-store": "^5.0.0",
    "electron-updater": "^4.1.2",
    "hasha": "^5.1.0",
    "luaparse": "^0.2.1",
    "moment": "^2.24.0",
    "regedit": "^3.0.3",
    "tail": "^2.0.3",
    "v-tooltip": "^2.0.2",
    "vue": "^2.6.10",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.14.1",
    "vue-router": "^3.1.3",
    "vue-toasted": "^1.1.27"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/register": "7.6.2",
    "@types/jest": "^24.0.18",
    "ajv": "^6.10.2",
    "babel-loader": "8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "cfonts": "^2.4.5",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.0",
    "css-loader": "^3.2.0",
    "del": "^5.1.0",
    "devtron": "^1.4.0",
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "0.8.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3",
    "url-loader": "^2.1.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  },
  "volta": {
    "node": "10.16.0",
    "yarn": "1.17.3"
  }
}
