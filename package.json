{
  "name": "weakauras-companion",
  "version": "4.1.0",
  "description": "WeakAuras Companion App",
  "author": "Buds <mrbouyou@gmail.com>",
  "scripts": {
    "build": "vue-cli-service electron:build --publish never",
    "compile-tools": "tsc -p tools",
    "dev": "vue-cli-service electron:serve",
    "dist": "vue-cli-service electron:build --publish onTagOrDraft",
    "i18n": "node ./tools/scripts/extract-i18n-messages.js",
    "lint": "eslint src/. --ext .js,.vue && npm run lint:scss",
    "lint:scss": "npx stylelint \"src/**/*.scss\"",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "archiver": "^5.3.1",
    "auto-launch": "^5.0.5",
    "axios": "^1.1.3",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "floating-vue": "^2.0.0-beta.20",
    "luaparse": "~0.2.1",
    "luxon": "^3.1.0",
    "material-icons": "^1.12.1",
    "pinia": "^2.0.23",
    "pinia-plugin-persistedstate-2": "^2.0.2",
    "regedit": "^5.1.1",
    "sharp": "^0.31.2",
    "tail": "^2.2.4",
    "tga": "^1.0.7",
    "vue": "^3.2.41",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/auto-launch": "^5.0.2",
    "@types/electron-localshortcut": "^3.1.0",
    "@types/jest": "^29.2.2",
    "@types/luaparse": "^0.2.8",
    "@types/luxon": "^3.1.0",
    "@types/sharp": "^0.31.0",
    "@types/tail": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vue/cli": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/eslint-config-typescript": "^10.0.0",
    "electron": "^21.2.2",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "glob": "^8.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.56.0",
    "sass-loader": "^13.2.0",
    "stylelint": "^14.14.1",
    "stylelint-config-recommended-scss": "^8.0.0",
    "typescript": "^4.8.4",
    "vue-cli-plugin-electron-builder": "^3.0.0-alpha.4",
    "vue-template-compiler": "^2.7.14"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "Electron >= 18.0.0"
  ],
  "engines": {
    "node": ">= 16"
  },
  "config": {
    "default-locale": "en",
    "supported-locales": [
      "en",
      "es",
      "de",
      "fr",
      "ru",
      "tr",
      "zh-cn"
    ]
  },
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/WeakAuras/WeakAuras-Companion.git"
  },
  "overrides": {
    "vue-cli-plugin-electron-builder": {
      "electron-builder": "^23.1.0"
    }
  }
}
