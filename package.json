{
  "name": "weakauras-companion",
  "version": "5.0.0",
  "description": "WeakAuras Companion App",
  "author": "Buds <mrbouyou@gmail.com>",
  "main": "dist/main/index.cjs",
  "license": "GPL-2.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/WeakAuras/WeakAuras-Companion.git"
  },
  "scripts": {
    "compile-tools": "tsc -p tools",
    "i18n": "node ./tools/scripts/extract-i18n-messages.js",
    "lint": "eslint packages/. --ext .ts,.js,.vue && npm run lint:scss",
    "lint:scss": "npx stylelint \"packages/**/*.scss\"",
    "dev": "node scripts/watch.mjs",
    "devtools": "./node_modules/.bin/vue-devtools",
    "prebuild": "vue-tsc --noEmit --p packages/renderer/tsconfig.json && node scripts/build.mjs",
    "build": "electron-builder",
    "test:e2e": "npx playwright test",
    "test:e2e:headless": "npx playwright test --headed"
  },
  "config": {
    "default-locale": "en",
    "supported-locales": [
      "en",
      "es",
      "de",
      "fr",
      "ru",
      "tr",
      "zh-cn"
    ]
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "regedit": "^5.1.1",
    "sharp": "^0.31.3",
    "tga": "^1.0.7"
  },
  "devDependencies": {
    "@electron/remote": "^2.0.9",
    "@intlify/unplugin-vue-i18n": "^0.8.1",
    "@playwright/test": "^1.29.2",
    "@types/archiver": "^5.3.1",
    "@types/auto-launch": "^5.0.2",
    "@types/electron-localshortcut": "^3.1.0",
    "@types/jest": "^29.2.5",
    "@types/luaparse": "0.2.1",
    "@types/luxon": "^3.2.0",
    "@types/sharp": "^0.31.1",
    "@types/tail": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/devtools": "^6.4.5",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^11.0.2",
    "axios": "^1.2.2",
    "electron": "20.2.0",
    "electron-builder": "^23.6.0",
    "electron-localshortcut": "^3.2.1",
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-vite": "^1.0.4",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "floating-vue": "^2.0.0-beta.20",
    "glob": "^8.0.3",
    "luaparse": "0.2.1",
    "luxon": "^3.2.1",
    "material-icons": "^1.13.1",
    "pinia": "^2.0.28",
    "pinia-plugin-persistedstate-2": "^2.0.8",
    "prettier": "^2.8.2",
    "sass": "^1.57.1",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended-scss": "^8.0.0",
    "tail": "^2.2.5",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.11.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-resolve": "^2.3.0",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-tsc": "^1.0.24"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue",
    "WeakAuras",
    "World of Warcraft"
  ],
  "overrides": {
    "minimatch": "^6.0.1"
  }
}