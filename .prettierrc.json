{
  "endOfLine": "auto",
  "$schema": "http://json.schemastore.org/prettierrc",
  "tabWidth": 2,
  "arrowParens": "always",
  "singleAttributePerLine": true,
  "bracketSameLine": false,
  "trailingComma": "all",
  "quoteProps": "consistent",
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrderTypeScriptVersion": "4.4.0",
  "importOrder": [
    "^(electron/(.*)$)|^(electron$)",
    "^(vue/(.*)$)|^(vue$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^~/(.*)$",
    "^[./]"
  ]
}
